.about{
  height: 270px;
  display: flex;
  background-color: #fff;
}
.aboutLeft{
  margin: 10px;
  width: 40vw;
  height: 250px;
}
.aboutRight{
  margin: 10px;
  width: 50vw;
  height: 250px;
}
.swiperImg{
  width: 100%;
  height: 100%;
}
.call{
  padding: 10px 0;
}
.call_card{
  width: 80vw;
  margin: 0 auto;
  display: flex;
  line-height: 40px;
  border-bottom: 1px solid #eee;
}
.call_head{
  width: 30%;
  text-align: right;
  padding-right: 10px;
}
.call_content{
  width: 70%;
  padding: 0 5px;
}